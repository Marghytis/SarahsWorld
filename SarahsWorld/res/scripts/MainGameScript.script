MAIN {
	scriptName = GAME_SETUP;
	scriptType = GAME_SETUP
}
GAME_SETUP {
	AVATAR_START_OFFSET = VEC: 0:1000
}
ATTRIBUTES {IMPOSSIBLE}
CHARACTERS {
	sarah = SARAH;
	meteor = MOVING_EFFECT
}
START {
	DO
		instantSpawn(meteor, sarah, METEOR, VEC: 400 : -100, 400);
		stick(sarah, meteor)
	~NEXT
		Wake_up
}
Wake_up {
	EMPTY
}