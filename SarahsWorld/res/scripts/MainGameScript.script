MAIN {
	scriptName = GAME_SETUP;
	scriptType = GAME_SETUP
}
GAME_SETUP {
	AVATAR_START_OFFSET = (VEC,0,1000));
	ZONETYPE = (STRING, VILLAGE))
}
CHARACTERS {
	sarah = SARAH;
	meteor = MOVING_EFFECT
}
START {
	DO
		spawn(meteor, sarah.link, sarah.pos, METEOR, (VEC, 400, -10), 400);
		stick(sarah, meteor)
	~NEXT
		Wake_up
}
Wake_up {
	EMPTY
}